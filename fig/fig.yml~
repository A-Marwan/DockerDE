mysql:
    image: jmoati/mysql-dev
    ports:
        - 127.0.0.1:3306:3306
        - 8080:80
    volumes:
        - ./data/mysql:/var/lib/mysql

elasticsearch:
    image: jmoati/elasticsearch-dev
    ports:
        - 9200:9200

rabbitmq:
    image: jmoati/rabbitmq-dev
    ports:
        - 127.0.0.1:5672:5672
        - 15672:15672

websrv:
    image: alveos/websrv
    ports:
        - 80:80
        - 443:443
    volumes:
        - .:/vagrant
    links:
        - elasticsearch:elasticsearch
        - mysql:mysql
        - rabbitmq:rabbitmq