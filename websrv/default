<VirtualHost *:80>
  <IfModule mod_rewrite.c>
    RewriteEngine on
    RewriteCond %{REQUEST_METHOD} ^(TRACE|TRACK)
    RewriteRule .* - [F]
  </IfModule>
  <Directory />
    Options FollowSymLinks
    AllowOverride All
  </Directory>
  DocumentRoot /vagrant/web
  <Directory /vagrant/web>
    Options Indexes FollowSymLinks MultiViews
    AllowOverride All
    Order allow,deny
    allow from all
  </Directory>
</VirtualHost>